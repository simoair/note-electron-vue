{ 
    "emmet.triggerExpansionOnTab": true,
    "files.associations": {
        "*.js": "javascriptreact",
        "*.vue": "vue"
    },

    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "html",
            "autoFix": true
        },
        {
            "language": "vue",
            "autoFix": true
        }
    ]
}